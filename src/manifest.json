{
  "manifest_version": 2,
  "name": "chrome-extension-boilerplate",
  "description": "",
  "version": "1.0.0",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "bundle.js"
      ],
      "css": [
      ],
      "run_at": "document_end"
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "tabs",
    "tabCapture",
    "http://*/*",
    "https://*/*"
  ]
}
